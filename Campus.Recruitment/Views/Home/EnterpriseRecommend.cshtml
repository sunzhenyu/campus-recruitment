        wechatLoginRediretUri =
        'http%3a%2f%2fwww.zheyibu.com%2fhome%2fWeChatLogin%2f%3freturnUrl%3dhttp%3a%2f%2fcompany.zheyibu.com%2f889561';
    $(function() {
        // 百度地图API功能
        var map = new BMap.Map("comMap");
        var top_left_control = new BMap.ScaleControl({
            anchor: BMAP_ANCHOR_TOP_LEFT
        }); // 左上角，添加比例尺
        var top_left_navigation = new BMap.NavigationControl(); //左上角，添加默认缩放平移控件
        //var top_right_navigation = new BMap.NavigationControl({ anchor: BMAP_ANCHOR_TOP_RIGHT, type: BMAP_NAVIGATION_CONTROL_SMALL }); //右上角，仅包含平移和缩放按钮
        map.enableScrollWheelZoom(true);
        map.addControl(top_left_control);
        map.addControl(top_left_navigation);
        //map.addControl(top_right_navigation);
        // 创建地址解析器实例
        var myGeo = new BMap.Geocoder();
        //设置地点坐标的文字说明
        var address = '荔湾区',
            city = "广州";
        $('.map-name').on('click', '.list', function() {
                address = $(this).attr('data-address');
                city = $(this).attr('data-city');
                myGeo.getPoint(address, function(point) {
                    if (point) {
                        map.centerAndZoom(point, 16);
                        map.addOverlay(new BMap.Marker(point));
                        var marker = new BMap.Marker(point); // 创建标注
                        map.addOverlay(marker);
                        var opts = {
                            width: 200, // 信息窗口宽度
                            height: 70, // 信息窗口高度
                            title: '就业联盟8', // 信息窗口标题
                            enableMessage: false //设置允许信息窗发送短息
                        }
                        var infoWindow = new BMap.InfoWindow(address, opts); // 创建信息窗口对象
                        marker.addEventListener("click", function() {
                            map.openInfoWindow(infoWindow, point); //开启信息窗口
                        });
                    } else {
                        alert("您选择地址没有解析到结果!");
                    }
                }, city);
            })
            // 将地址解析结果显示在地图上,并调整地图视野
        myGeo.getPoint(address, function(point) {
            if (point) {
                map.centerAndZoom(point, 16);
                map.addOverlay(new BMap.Marker(point));
                var marker = new BMap.Marker(point); // 创建标注
                map.addOverlay(marker);
                var opts = {
                    width: 200, // 信息窗口宽度
                    height: 70, // 信息窗口高度
                    title: "就业联盟8校园", // 信息窗口标题
                    enableMessage: false //设置允许信息窗发送短息
                }
                var infoWindow = new BMap.InfoWindow(address, opts); // 创建信息窗口对象
                marker.addEventListener("click", function() {
                    map.openInfoWindow(infoWindow, point); //开启信息窗口
                });
            } else {
                alert("公司地址没有解析到结果!");
            }
        }, city);
    });
</script>